<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" 
    android:layout_width="fill_parent" 
    android:layout_height="fill_parent"
    android:id="@+id/switcher"> 
    
	<ImageView
			android:id="@+id/home"
			android:layout_width="84px"
			android:layout_height="84px"
			android:background="@drawable/icon_home"
			android:layout_marginLeft="20px"
			android:layout_marginTop="20px"
			/>	
        
	<ViewStub
  		android:id="@+id/spcviewstub_list"

  		android:layout="@layout/spc_progress_overlay"

  		android:layout_width="600px"
  		android:layout_height="wrap_content"
  		android:layout_marginTop="380px"
  		android:layout_marginLeft="10px"
  		android:layout_gravity="bottom" 
  		android:gravity="bottom"/>    
  		    
	<ImageView
			android:id="@+id/spcmaintitle"
			android:layout_width="363px"
			android:layout_height="67px"
			android:layout_marginLeft="250px"
			android:layout_marginTop="390px"
			/>	      
			

	<ImageView
			android:id="@+id/spclisttitle"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="10px"
			android:layout_marginTop="290px"
			/>	     
	
	<ViewStub
  		android:id="@+id/comviewstub_list"

  		android:layout="@layout/com_progress_overlay"

  		android:layout_width="600px"
  		android:layout_height="wrap_content"
  		android:layout_marginTop="350px"
  		android:layout_marginLeft="10px"
  		android:layout_gravity="bottom" 
  		android:gravity="bottom"/>    
  					
	<ImageView
			android:id="@+id/maintitle"
			android:layout_width="363px"
			android:layout_height="67px"
			android:layout_marginLeft="20px"
			android:layout_marginTop="390px"
			/>	      
			

	<ImageView
			android:id="@+id/listtitle"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="10px"
			android:layout_marginTop="280px"
			/>	  
						

			  
    <ListView 
    	android:id="@+id/listview"
        android:background="#55000000"
        android:layout_width="180px"
        android:layout_height="fill_parent"
        android:layout_alignParentRight="true"
        android:gravity="center"
        android:layout_gravity="center"
        android:cacheColorHint="#00000000"
        
    />

</RelativeLayout>
   
